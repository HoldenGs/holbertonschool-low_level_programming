#include "holberton.h"

/**
 * strcmp - compare two strings
 *
 * @s1: first string to compare with @s2
 *
 * @s2: second string from which to compare @s1
 *
 * Return: integer value of characters of @s1 minus character number of @s2
 */
int _strcmp(char *s1, char *s2)
{
	int n = 0, m = 0;

	while (s1[n])
		n += s1[n];
	while (s2[m])
		m += s2[m];
	return (n - m);
}
